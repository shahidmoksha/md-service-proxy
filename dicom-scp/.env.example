# AE Title for this SCP (receiver)
AE_TITLE=CLEANSCP

# Port to listen on for incoming C-STORE requests
PORT=1040

# Target DICOM AE Title (forwarding destination)
TARGET_AE=LCHBLR

# Host and port of the target DICOM server
TARGET_HOST=127.0.0.1
TARGET_PORT=11112

# Number of retries when forwarding fails
MAX_RETRIES = 3

# Delay between retries (in seconds)
RETRY_DELAY=2

# Idle queue log interval (10x multiplier in seconds)
LOG_DELAY=30

# Number of background worker threads
NUM_WORKERS=4
